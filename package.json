    "@codemirror/theme-one-dark": "^6.1.0",
    "@codemirror/view": "^6.7.3",
    "@fontsource/electrolize": "^4.5.8",
    "@fontsource/lexend-deca": "^4.5.14",
    "@fontsource/manrope": "^4.5.13",
    "@kubernetes/client-node": "^0.18.1",
    "@supabase/auth-helpers-shared": "^0.2.4",
    "@supabase/supabase-js": "^2.7.0",
    "amqplib": "^0.10.3",
