{
  "name": "skf-dashboard",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check:ts": "tsc --noEmit",
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.svelte,.astro",
    "generate:json": "tsx ./src/scripts/fetchLabs.ts",
    "start:server": "node ./dist/server/entry.mjs",
    "rabbitmq": "docker run -it --rm --name rabbitmq -p 5672:5672 -p 15672:15672 rabbitmq:3.11-management",
    "update-types": "npx supabase gen types typescript --project-id \"ccsgfooankckfqpmcfyb\" > src/db/database.types.d.ts",
    "deno:deployment": "deno run -A ./src/workers/deployment.ts",
    "deno:kubernetes": "deno run -A ./src/workers/kubernetes.ts"
  },
  "dependencies": {
    "@astrojs/svelte": "^2.0.1",
    "@astrojs/vercel": "^3.0.1",
